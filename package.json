{
  "name": "web-frontend-boilerplate",
  "version": "0.0.0-semantically-released",
  "description": "A boilerplate for developing frontend UI application",
  "keywords": [
    "boilerplate",
    "frontend",
    "web-frontend",
    "frontend-ui"
  ],
  "homepage": "https://github.com/dwiyatci/web-frontend-boilerplate",
  "bugs": "https://github.com/dwiyatci/web-frontend-boilerplate/issues",
  "license": "SEE LICENSE IN LICENSE.txt",
  "author": "Glenn Dwiyatcita <glenn@nonpartisan.com> (https://tiny.cc/dwiyatci)",
  "repository": {
    "type": "git",
    "url": "https://github.com/dwiyatci/web-frontend-boilerplate.git"
  },
  "scripts": {
    "install:clean": "yarn upgrade || yarn install",
    "update": "yarn upgrade-interactive --latest",
    "start": "webpack-dev-server --hot",
    "build": "yarn webpack --env.production --progress",
    "webpack": "yarn clean && webpack",
    "clean": "rimraf assets",
    "test": "echo \"There is no secret to writing tests... ... there are only secrets to writing testable code!\"",
    "lint": "eslint --cache src",
    "prettier": "prettier --write",
    "semantic-release": "semantic-release",
    "semantic-release-dry-run": "semantic-release --dry-run",
    "commit": "git cz"
  },
  "dependencies": {
    "@babel/polyfill": "7.2.5",
    "@babel/runtime-corejs2": "7.2.0",
    "bootstrap": "4.2.1",
    "font-awesome": "4.7.0",
    "jquery": "3.3.1",
    "lodash": "4.17.11",
    "popper.js": "1.14.6",
    "tether": "1.4.5"
  },
  "devDependencies": {
    "@babel/core": "7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "7.2.0",
    "@babel/plugin-transform-runtime": "7.2.0",
    "@babel/preset-env": "7.2.3",
    "babel-eslint": "10.0.1",
    "babel-loader": "8.0.5",
    "babel-plugin-lodash": "3.3.4",
    "bootstrap-loader": "3.0.2",
    "commitizen": "3.0.5",
    "css-loader": "2.1.0",
    "cz-conventional-changelog": "2.1.0",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "3.3.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.3",
    "exports-loader": "0.7.0",
    "file-loader": "3.0.1",
    "font-awesome-loader": "1.0.2",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "4.0.0-beta.5",
    "imports-loader": "0.8.0",
    "inline-manifest-webpack-plugin": "4.0.2",
    "less": "3.9.0",
    "mini-css-extract-plugin": "0.5.0",
    "node-sass": "4.11.0",
    "postcss": "7.0.8",
    "postcss-load-config": "2.0.0",
    "postcss-loader": "3.0.0",
    "prettier": "1.15.3",
    "resolve-url-loader": "3.0.0",
    "rimraf": "2.6.3",
    "sass-loader": "7.1.0",
    "semantic-release": "15.13.2",
    "style-loader": "0.23.1",
    "url-loader": "1.1.2",
    "webpack": "4.28.1",
    "webpack-cli": "3.2.1",
    "webpack-dev-server": "3.1.14"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  }
}
